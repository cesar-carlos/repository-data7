{
  "name": "repository-data7",
  "version": "1.0.0",
  "description": "",
  "main": "./src/main.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only ./src/main.ts",
    "dev:nodemon": "cls && nodemon --watch 'src/' --exec 'ts-node --files --transpile-only src/maints' -e ts",
    "format": "npx prettier './src/**/*.ts' './src/**/*.js' --write",
    "build": "tsc",
    "build:wind": "tsc && copyfiles ./.env ./dist/src/ && copyfiles ./src/certificates/**/* ./dist/ && copyfiles ./src/sql/**/* ./dist/",
    "start": "node ./dist/main.js",
    "test": "jest --watchAll --config ./jest.config.js"
  },
  "keywords": [
    "Data7"
  ],
  "author": "cesar carlos",
  "license": "MIT",
  "files": [
    "/dist"
  ],
  "dependencies": {
    "dotenv": "^16.0.1",
    "moment": "^2.29.3",
    "mssql": "^8.1.2",
    "promisify": "^0.0.3",
    "sybase": "^1.2.3",
    "tedious": "^14.6.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/mssql": "^8.0.3",
    "@types/node": "^18.0.0",
    "@types/tedious": "^4.0.8",
    "@types/uuid": "^8.3.4",
    "copyfiles": "^2.4.1",
    "jest": "^28.1.2",
    "ts-jest": "^28.0.5",
    "@types/jest": "^28.1.4",
    "nodemon": "^1.3.3",
    "prettier": "2.7.1",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}